#include <stdio.h>

int main() {
    int age;

    // Get age input from the user
    printf("Enter your age: ");
    scanf("%d", &age);

    // Check if age is 18 or more
    if (age >= 18) {
        printf("You are an adult.\n");
    } else {
        printf("You are not an adult.\n");
    }

    return 0;
}
