#include <stdio.h>

int main() {
    int num, sum = 0;

    do {
        printf("Enter a positive number (0 or negative to stop): ");
        scanf("%d", &num);

        if (num > 0) {
            sum += num;
        }
    } while (num > 0);

    printf("Sum of positive numbers: %d\n", sum);

    return 0;
}
