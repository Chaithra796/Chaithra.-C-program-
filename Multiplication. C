#include <stdio.h>

// Function declaration (also called function prototype)
int multiply(int a, int b);

int main() {
    int num1, num2, result;

    printf("Enter two numbers: ");
    scanf("%d %d", &num1, &num2);

    // Function call
    result = multiply(num1, num2);

    printf("Product = %d\n", result);

    return 0;
}

// Function definition
int multiply(int a, int b) {
    return a * b;
}
